
import java.util.*;
import java.lang.*;

 class GfG
 {
	public static int firstDigit(int n)
	{
		while (n >= 10)
			n /= 10;
		return n;
	}
	public static int lastDigit(int n)
	{
		return (n % 10);
	}
	public static void main(String argc[])
	{
	    
		int n,t;
		Scanner sc=new Scanner(System.in);
        t=sc.nextInt();
        for(int i=1; i<=t; i++)
        {
            n=sc.nextInt();
		    System.out.println(firstDigit(n) + lastDigit(n));
        }
	}
}

