import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception,NoSuchFieldException
	{
		// your code goes here
		
	    float amount;
        float total_amount;
      
        

        Scanner sc1= new Scanner(System.in);
        amount = sc1.nextFloat();
        total_amount=sc1.nextFloat();
       

        if(total_amount<amount+0.50){
            System.out.print(total_amount);

        }
        else if(amount%5==0 ) {
            

            System.out.print(total_amount-amount-0.50);



        }
       
        else {

            System.out.print(total_amount);

        }


		
		
		
		
		
		
	}
}
